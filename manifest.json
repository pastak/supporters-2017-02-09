{
  "name": "Supporterzを便利にするやつ",
  "description": "Supporterzを便利にするやつ",
  "author": "pastak",
  "version": "0.0.1",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": ["https://supporterz.jp/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "browser_action": {},
  "manifest_version": 2
}
